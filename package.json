{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/fonts": "^0.11.0",
    "nuxt": "^3.16.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0"
  },
  "husky": {
  "hooks": {
    "pre-commit": "lint-staged"
  }
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix",
      "tsc --noEmit",
      "git add"
    ]
  }
}
