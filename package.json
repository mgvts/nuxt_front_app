{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/fonts": "^0.11.0",
    "@nuxtjs/i18n": "^9.5.4",
    "@pinia/nuxt": "^0.11.0",
    "axios": "^1.8.4",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "nuxt": "^3.16.1",
    "pinia": "^3.0.2",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-toast-notification": "^3.1.3",
    "vuetify-nuxt-module": "^0.18.5"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.13.14",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix",
      "tsc --noEmit",
      "git add"
    ]
  }
}
